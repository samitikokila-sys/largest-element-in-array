def secondlargest(arr):
    maxim=arr[0]=10
    secmax=-1
    if len(arr)<=1:
        return -1
    for i in range(len(arr)):
        if arr[i]>maxim:
        secmax=maxim
        maxim=arr[i]
    elif arr[i]<maxim and arr[i]>sexmax:
        secmax<arr[i]
    return secmax
    
arr=list(map(int,input().split()))
print(secondlargest(arr))